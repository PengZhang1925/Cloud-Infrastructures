<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AdminViewController">
   <children>
      <ComboBox fx:id="CandidateName" layoutX="242.0" layoutY="372.0" prefHeight="30.0" prefWidth="150.0" promptText="CandidateName" />
      <Label layoutX="218.0" layoutY="313.0" prefHeight="50.0" prefWidth="199.0" text="Select the Candidate Name:" textFill="#158686" wrapText="true">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Label layoutX="179.0" layoutY="36.0" prefHeight="60.0" prefWidth="350.0" text="Check &amp; Send Bill !!" textFill="#1214c7">
         <font>
            <Font name="Arial Bold" size="36.0" />
         </font>
      </Label>
      <Label layoutX="298.0" layoutY="423.0" text="Votes:">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Label>
      <Label fx:id="votes" layoutX="242.0" layoutY="451.0" prefHeight="30.0" prefWidth="150.0" />
      <Button layoutX="267.0" layoutY="488.0" mnemonicParsing="false" onAction="#CheckVotes" prefHeight="30.0" prefWidth="100.0" text="Check" />
      <Button layoutX="267.0" layoutY="586.0" mnemonicParsing="false" onAction="#Back" prefHeight="50.0" prefWidth="100.0" text="LogOut" />
      <Button layoutX="199.0" layoutY="144.0" mnemonicParsing="false" onAction="#StartVoting" prefHeight="100.0" prefWidth="100.0" text="Start Voting" textFill="#ee0d24">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="335.0" layoutY="144.0" mnemonicParsing="false" onAction="#CollectVotes" prefHeight="100.0" prefWidth="100.0" text="Collect Votes" textFill="#f50606">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="raw" layoutX="267.0" layoutY="277.0" prefHeight="30.0" prefWidth="117.0" text="Raw message here">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
